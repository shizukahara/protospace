.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.active{ role: "presentation" }
          = link_to 'Popular PROTO', '#'
        %li{ role: "presentation" }
          = link_to 'Newest PROTO', '#'
      = button_tag 'View Tags', type: "button", class: "btn btn-default col-lg-1"




.container.proto-list
  .row
    = render @prototypes

.text-center
  %ul.pagination
    %li.disabled
      %a{ area: { label: 'Previous' }, href: '#' }
        %span{ aria: { hidden: 'true' }} «
    %li.active
      = link_to '1', '#'
    %li
      = link_to '2', '#'
    %li
      = link_to '3', '#'
    %li
      = link_to '4', '#'
    %li
      = link_to '5', '#'
    %li
      %a{ area: { label: 'Next' }, href: '#' }
        %span{ aria: { hidden: 'true' }} »




